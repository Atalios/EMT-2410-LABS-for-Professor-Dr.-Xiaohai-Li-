#include <iostream>
using namespace std;
int main()
{
int times;
cout << "Enter an integer: \n";
cin >> times;
for (int i = 0;i<times;i++)
{
cout << "Hello World!\n";// prints !!!Hello World!!!
}
return 0;
}
